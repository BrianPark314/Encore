    "def sangun(n, i):\n",
    "    if n>5:\n",
    "        i += 1\n",
    "        sangun(n-5, i)\n",
    "    elif n!=3:\n",
    "        return -i\n",
    "    else:\n",
    "        i += 1\n",
    "    return n, i\n",
    "\n",
    "print(sangun(18,0))"
   ]
